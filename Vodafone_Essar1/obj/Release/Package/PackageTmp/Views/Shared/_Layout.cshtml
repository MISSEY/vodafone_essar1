		<script type="text/javascript">
		    $(function () {

		        document.getElementById('upload_type').disabled = true;
           var error_1 = '@error_1';

		        if (!error_1 == '') {
		            alert(error_1);
		           
                    @{
		            ViewBag.Error_SimandMobile = null; }
		        }
			$('#supported').text('Supported/allowed: ' + !!screenfull.enabled);

			if (!screenfull.enabled) {
				return false;
			}

	
			    $('.dropdown-submenu a.test').on("click", function (e) { 
			    $(this).next('ul').toggle();
			    e.stopPropagation();
			    e.preventDefault();
			   
			});

			$('#toggle').click(function () {
				screenfull.toggle($('#container')[0]);
			});
			
			$("#inputfile").click(function () {

			    document.getElementById("button_choose").style.display = "block";

			});

			$("#Choose_file").on("click", "li", (function () {
			    document.getElementById('upload_type').disabled = false;
			    var i = $(this).index();			   
			    document.getElementById("upload_type").value = i;

			    })
            );
			$("#button_choose").on("click", (function () {
			   
			    var filepath = document.getElementById("inputfile").value;
			   
			    if (filepath == "") {
			        alert("Please Choose Excel File To Upload");
			    }

			})
            );

			$("#Template_download_Sim").on("click", (function () {

			    $.ajax({
			        url: '@Url.Action("Template_download1", "File")',
	                success: function (result) {
	                    window.location = '@Url.Action("Template_download1", "File")';
	                }
	            });


			}));
		    $("#Template_download_Mob").on("click", (function () {

		        $.ajax({
		            url: '@Url.Action("Template_download2", "File")',
			        success: function (result) {
			            window.location = '@Url.Action("Template_download2", "File")';
	                }
			    });


            }));

	                

		});
		
		   
		 

		
		</script>
    

